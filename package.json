{
  "name": "dubhe-app-admin-api",
  "version": "1.0.0",
  "description": "移动管理APP后台管理API",
  "private": true,
  "dependencies": {
    "egg": "^2.4.1",
    "egg-scripts": "^2.5.0",
    "egg-sequelize": "^3.1.5",
    "egg-validate": "^1.0.0",
    "tedious": "^2.6.3"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-dubhe-app-admin-api",
    "start-docker": "egg-scripts start --port 80 --title=egg-server-dubhe-app-admin-api",
    "stop": "egg-scripts stop --title=egg-server-dubhe-app-admin-api",
    "dev": "egg-bin dev",
    "debug": "export EGG_AGENT_DEBUG_PORT=8900 && egg-bin debug --port 7002 --proxy=9989",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
